        .fa-level-up:before{content:"\f148"}
        .fa-long-arrow-right:before{content:"\f178"}

        .fa-long-arrow-right {
            padding-right: 5px;
        }

        .fa-level-up {
            transform: rotate(90deg);
            padding-right: 5px;
            padding-top: 0px;
            vertical-align: bottom;
        }

        .ml-client-list-container, .ml-relclient-list-container {
