@model PracticeStrategy.PricingCentral.UI.Models.ClientModel
@using System.Configuration
<!DOCTYPE html>
@{
    Layout = "";
    string currentUrl = Request.Url.GetLeftPart(UriPartial.Authority) + "/";
    string personUrl = ConfigurationManager.AppSettings["personUrl"];
}

<html>
<head>
    <style>
        .container-fluid {
            margin: 15px;
            padding: 5px;
        }

        .ml-top-nav {
            height: 60px;
            padding: 10px 0;
            border-radius: 0;
            margin-bottom: 20px;
            border-color: #93D8F7;
            background-color: #93D8F7;
        }                        

        .ml-top-nav-image {
            width: 300px;
            vertical-align: middle;
            border: none;
            margin-top: 10px;
        }

        hr {
            margin: 10px 0px;
        }

        .col-lg-9, .col-lg-3 {
            display: inline-block;
        }

        .col-lg-9 {
            width: 80%;
        }

            .col-lg-9 h1 {
                margin-top: -8px;
            }

        .ml-relatedclient-block {
            background-color: #452660;
            color: WHITE;
            padding: 5PX 10PX;
            display: INLINE;
        }

        .margin-10 {
            margin: 5px;
        }

        .ml-txt-18 {
            font-size: 18px;
        }

        .ml-txt10 {
            font-size: 30px;
        }

        .ml-header-title {
            font-size: 14px;
            font-weight: bold;
            margin-bottom: 10px;
        }

        .ml-attorney.ml-profile-photo.ml-square-container-md img.image-replacement {
            padding-left: 100%;
            height: 100%;
        }

        .image-replacement {
            padding-left: 87px;
            height: 80px;
            background: #333 url('@currentUrl/Images/no-img.jpg') no-repeat center;
            background-size: cover;
        }

        .ml-square-container-md {
            width: 110px;
            height: 160px;
            overflow: hidden;
            display: inline-block;
        }

        .ml-profile-photo {
            float: left;
            background-position: center center;
            background-size: cover;
            margin-right: 15px;
        }

        .ml-sub-content-container {
            padding: 0px 15px;
        }

        .pull-right {
            float: right;
        }

        @@font-face {
          font-family: 'Glyphicons Halflings';

          src: url('@currentUrl/fonts/glyphicons-halflings-regular.eot');
          src: url('@currentUrl/fonts/glyphicons-halflings-regular.eot?#iefix') format('embedded-opentype'), url('@currentUrl/fonts/glyphicons-halflings-regular.woff2') format('woff2'), url('@currentUrl/fonts/glyphicons-halflings-regular.woff') format('woff'), url('@currentUrl/fonts/glyphicons-halflings-regular.ttf') format('truetype'), url('@currentUrl/fonts/glyphicons-halflings-regular.svg#glyphicons_halflingsregular') format('svg');
        }
        .glyphicon {
          position: relative;
          top: 1px;
          display: inline-block;
          font-family: 'Glyphicons Halflings';
          font-style: normal;
          font-weight: normal;
          line-height: 1;

          -webkit-font-smoothing: antialiased;
          -moz-osx-font-smoothing: grayscale;
        }

        .glyphicon-phone-alt:before {
          content: "\e183";
        }
        .glyphicon-phone:before {
          content: "\e145";
        }

        @@font-face{
        font-family:'FontAwesome';
